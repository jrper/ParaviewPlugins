<ServerManagerConfiguration>
  <ProxyGroup name="filters">
    <SourceProxy name="vtkCVVelocities" class="vtkCVVelocities" label="Velocities on CV Faces">
      <Documentation long_help="Plot pointwise velocity on midpoints of Control Volume faces" short_help="Plot CV Vels">
      </Documentation>
      <InputProperty
	  name="Input"
	  command="SetInputConnection">
	<ProxyGroupDomain name="groups">
	  <Group name="sources"/>
	  <Group name="filters"/>
	</ProxyGroupDomain>
	<DataTypeDomain name="input_type">
	  <DataType value="vtkUnstructuredGrid"/>
	</DataTypeDomain>
      </InputProperty>
      <IntVectorProperty
          name="Polynomial Degree"
          command="SetDegree"
          number_of_elements="1"
          default_values="-1">
        <EnumerationDomain name="enum">
          <Entry value="-1" text="Unchanged"/>
          <Entry value="1" text="Linear"/>
          <Entry value="2" text="Quadratic"/>
        </EnumerationDomain>
        <Documentation>
          Set the degree of the output grid.
        </Documentation>
      </IntVectorProperty>
      <IntVectorProperty
          name="Continuity"
          command="SetContinuity"
          number_of_elements="1"
          default_values="0">
        <EnumerationDomain name="enum">
          <Entry value="0" text="Unchanged"/>
          <Entry value="-1" text="Discontinuous"/>
          <Entry value="1" text="Continuous"/>
        </EnumerationDomain>
        <Documentation>
          Set the continuity of the output grid.
        </Documentation>
      </IntVectorProperty>
    </SourceProxy>
  </ProxyGroup>
</ServerManagerConfiguration>
